@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase
@{
    Layout = null;
}
<div class="con-center">
    <div class="pure-g">
        <div class="pure-u-1-2">
            <label class="pure-u-1-1">编号: </label>
            <label class="pure-u-1-1">{{item.id}} </label>
        </div>
        <div class="pure-u-1-2">
            <label class="pure-u-1-1">计划名称: </label>
            <label class="pure-u-1-1">{{item.sched_name}} </label>
        </div>
        <div class="pure-u-1-2">
            <label class="pure-u-1-1">作业名称: </label>
            <label class="pure-u-1-1">{{item.job_name}} </label>
        </div>
        <div class="pure-u-1-2">
            <label class="pure-u-1-1">作业组: </label>
            <label class="pure-u-1-1">{{item.job_group}} </label>
        </div>
        <div class="pure-u-1-2">
            <label class="pure-u-1-1">外部程序集: </label>
            <label class="pure-u-1-1">{{item.outAssembly}} </label>
        </div>
        <div class="pure-u-1-2">
            <label class="pure-u-1-1">调用类: </label>
            <label class="pure-u-1-1">{{item.job_class_name}} </label>
        </div>
        <div class="pure-u-1-2">
            <label class="pure-u-1-1">是否可用: </label>
            <label class="pure-u-1-1">{{item.is_durable | boolFilter}} </label>
        </div>
        <div class="pure-u-1-2">
            <label class="pure-u-1-1">描述: </label>
            <label class="pure-u-1-1">{{item.description}} </label>
        </div>
        <div class="pure-u-1-2">
            <label class="pure-u-1-1">开始时间: </label>
            <label class="pure-u-1-1">{{item.startTime}} </label>
        </div>
        <div class="pure-u-1-2">
            <label class="pure-u-1-1">结束时间: </label>
            <label class="pure-u-1-1">{{item.endTime}} </label>
        </div>
        <p>
            <button class="pure-button pure-button-primary"
                    ng-disabled="isVoted" ng-click="vote()">
                {{voteBtnText}}
            </button>
            <a href="#!/list" class="pure-button">返回</a>
        </p>
    </div>
</div>